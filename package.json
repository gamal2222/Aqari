{
  "name": "aqari",
  "version": "2.0.0",
  "description": "Premium Property Rental and Sales Mobile App - Advanced Real Estate Platform with Payments, Messaging, Analytics & More",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test e2e --configuration ios.sim.debug --cleanup",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "pod-install": "cd ios && pod install && cd ..",
    "pod-update": "cd ios && pod repo update && pod update && cd ..",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:all": "rm -rf node_modules package-lock.json && npm install && npm run pod-install && npm run clean:android",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Pods Podfile.lock build && pod install && cd ..",
    "build:android:debug": "cd android && ./gradlew assembleDebug && cd ..",
    "build:android:release": "cd android && ./gradlew bundleRelease && cd ..",
    "build:ios:debug": "cd ios && xcodebuild -workspace Aqari.xcworkspace -scheme Aqari -configuration Debug -derivedDataPath build && cd ..",
    "build:ios:release": "cd ios && xcodebuild -workspace Aqari.xcworkspace -scheme Aqari -configuration Release -derivedDataPath build && cd ..",
    "prebuild": "expo prebuild --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "eas:build": "eas build",
    "eas:build:android": "eas build --platform android",
    "eas:build:ios": "eas build --platform ios",
    "eas:submit": "eas submit",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run test",
    "analyze": "react-native-bundle-visualizer",
    "storybook": "start-storybook -p 7001",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.4",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/drawer": "^6.6.8",
    "react-native-screens": "^3.26.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-elements": "^3.4.3",
    "react-native-paper": "^5.11.4",
    "react-native-vector-icons": "^10.0.0",
    "react-native-svg": "^13.14.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/auth": "^18.3.0",
    "@react-native-firebase/database": "^18.3.0",
    "@react-native-firebase/storage": "^18.3.0",
    "@react-native-firebase/messaging": "^18.3.0",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-firebase/firestore": "^18.3.0",
    "@react-native-firebase/performance": "^18.3.0",
    "@react-native-firebase/crashlytics": "^18.3.0",
    "react-native-maps": "^1.7.1",
    "@stripe/stripe-react-native": "^0.31.0",
    "formik": "^2.4.5",
    "yup": "^1.3.3",
    "axios": "^1.6.2",
    "redux": "^4.2.1",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.10",
    "react-native-image-viewing": "^0.2.2",
    "react-native-image-resizer": "^1.2.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-blur": "^4.3.0",
    "react-native-dotenv": "^3.3.1",
    "moment": "^2.29.4",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "expo": "^49.0.15",
    "expo-notifications": "^0.20.1",
    "expo-permissions": "^14.4.0",
    "expo-constants": "^15.4.6",
    "expo-device": "^5.4.0",
    "expo-updates": "^0.24.12",
    "socket.io-client": "^4.5.4",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-geocoding": "^0.5.0",
    "react-native-permissions": "^3.10.0",
    "react-native-swiper": "^1.6.0",
    "react-native-video": "^5.2.1",
    "react-native-sound": "^0.11.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-rate": "^1.2.10",
    "react-native-share": "^10.0.0",
    "react-native-device-info": "^10.11.0",
    "react-native-netinfo": "^11.3.1",
    "react-native-keychain": "^8.1.2",
    "react-native-sensitive-info": "^6.1.3",
    "react-native-code-push": "^8.0.1",
    "lottie-react-native": "^6.4.1",
    "bottom-sheet": "^4.4.101",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/datetimepicker": "^7.6.0",
    "react-native-calendars": "^1.1405.0",
    "react-native-tab-view": "^3.2.1",
    "react-native-pager-view": "^6.2.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-collapsible": "^1.6.1",
    "react-native-modal": "^13.0.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-super-grid": "^4.2.0",
    "react-native-splash-screen": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.5",
    "babel-jest": "^29.7.0",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "eslint": "^8.54.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "@types/react-native": "^0.72.8",
    "@types/react": "^18.2.37",
    "@types/react-redux": "^7.1.27",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "react-native-cli": "^2.0.1",
    "@react-native-community/cli": "^12.0.0",
    "@react-native-community/cli-platform-android": "^12.0.0",
    "@react-native-community/cli-platform-ios": "^12.0.0",
    "detox": "^20.16.3",
    "detox-cli": "^20.16.3",
    "detox-test-utils": "^1.1.5",
    "@storybook/react-native": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "react-native-bundle-visualizer": "^3.5.0",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/",
      "/e2e/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|@react-native-firebase|@react-native-community|@stripe|react-native-maps|react-native-geolocation-service|react-native-permissions)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/navigation/**",
      "!src/types/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-native": "^0.72.4"
  },
  "keywords": [
    "react-native",
    "property",
    "rental",
    "sales",
    "mobile-app",
    "firebase",
    "stripe",
    "real-estate",
    "payments",
    "messaging",
    "analytics",
    "notifications",
    "geolocation",
    "redux",
    "typescript"
  ],
  "author": "Gamal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gamal2222/Aqari.git"
  },
  "bugs": {
